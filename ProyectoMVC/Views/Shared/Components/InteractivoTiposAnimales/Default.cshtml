@{
}
<style>
    .tipoAnimales {
        border: solid 1px #aaa;
        border-radius: 10px;
        padding: 12px;
    }

        .tipoAnimales .tipoAnimales {
            display: inline-block;
            float: left;
            max-width: 200px;
            font-size: 12px;
        }

    .clearer {
        clear: both;
    }
</style>

<div class="tipoAnimales">
    <p>Tipos de animal</p>

    @foreach(var item in Model)
    {
        <div class"animales m-2">
            <h4>@item.Key</h4>
            <p> @item.Value</p>
        </div>  
    }
    <div class="clearer"></div>
</div>